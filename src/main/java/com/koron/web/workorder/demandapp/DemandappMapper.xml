  <select id="queryList" parameterType="com.koron.web.workorder.demandapp.bean.DemandappQueryBean" resultMap="BaseResultMap">
    select s.name as staffunName,d.name as depatName,ws.name as auditorName,demandapp.*
    from demandapp
    left join tblstaffun s on s.code=demandapp.employee_id
    left join tbldepartun d on d.code=demandapp.depat_code
    left join tblstaffun ws on ws.workflow_code=demandapp.auditor
    where 1=1 and remove_flag = 0
    <if test="beginDate  != null and beginDate != ''">
      and DATE_FORMAT( bill_date, '%Y-%m-%d') &gt;= DATE_FORMAT(#{beginDate}, '%Y-%m-%d')
    </if>
    <if test="endDate != null and endDate != ''">
      and DATE_FORMAT( bill_date, '%Y-%m-%d') &lt;= DATE_FORMAT(#{endDate}, '%Y-%m-%d')
    </if>
    <if test="staffunName != null and staffunName !=''">
      and s.name like CONCAT('%', #{staffunName}, '%')
    </if>
    <if test="depatName != null and depatName !=''">
      and d.name like CONCAT('%', #{depatName}, '%')
    </if>
    <if test="appsysName != null and appsysName !=''">
      and demandapp.appsys_name like CONCAT('%', #{appsysName}, '%')
    </if>
    <if test="status != null ">
      and demandapp.status = #{status}
    </if>
    <if test="orderNo != null and orderNo !=''">
      and demandapp.order_no like CONCAT('%', #{orderNo}, '%')
    </if>
    <if test="logincode != null and logincode!='' and broper!='all'">
      and (demandapp.create_account = #{logincode,jdbcType=VARCHAR} || demandapp.employee_id = #{logincode,jdbcType=VARCHAR})
    </if>
    order by demandapp.bill_date desc,demandapp.create_time desc
  </select>

  <select id="selectByPrimaryKey" parameterType="java.lang.String" resultMap="BaseResultMap">
    select s.name as staffunName,d.name as depatName,ws.name as auditorName,demandapp.*
    <!--<include refid="Base_Column_List" />-->
    from demandapp
    left join tblstaffun s on s.code=demandapp.employee_id
    left join tbldepartun d on d.code=demandapp.depat_code
    left join tblstaffun ws on ws.workflow_code=demandapp.auditor
    where 1=1 and remove_flag = 0
    and demandapp.id = #{id,jdbcType=VARCHAR}
  </select>

